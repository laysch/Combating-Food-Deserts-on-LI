            <Label />
            <TextField fx:id="cityField" promptText="City" />
            <Label />
            <TextField fx:id="stateField" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="272.0" promptText="State" />
            <Label />
            <TextField fx:id="zipField" promptText="Zipcode" />
            <Label />
            <TextField fx:id="hoursField" promptText="Hours 'M-F 6am-10pm'" />
            <Label />
            <TextField fx:id="urlField" prefHeight="25.0" prefWidth="550.0" promptText="Url" />
         </children>
      </VBox>
      <Button layoutX="369.0" layoutY="457.0" mnemonicParsing="false" onAction="#resetFields" prefHeight="25.0" prefWidth="98.0" text="Reset" />
      <Label alignment="CENTER" layoutX="224.0" layoutY="66.0" prefHeight="31.0" prefWidth="243.0" text="Add Data">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="224.0" layoutY="457.0" mnemonicParsing="false" onAction="#addData" prefHeight="25.0" prefWidth="98.0" text="Write" />
   </children>
